{
    "manifest_version": 2,
    "name": "CDA Downloader",
    "version": "0.1",
    "description": "Rozszerzenie pobiera aktualnie oglądany filmik nadając mu tytuł. ",
    "icons": {
      "16": "img/icon128.png",
      "48": "img/icon128.png",
      "128": "img/icon128.png"
    },
    "author": "Igifigi",
    "browser_action": {
      "default_icon": "img/icon128.png"
    },
    "minimum_chrome_version": "16.0.884",
    "permissions": ["<all_urls>", "tabs", "downloads"],
    "background": {
      "scripts": ["background.js"]
    }
  }
  